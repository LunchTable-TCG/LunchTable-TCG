{
  "name": "@lunchtable/web-tanstack",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "elizaos": {
    "kind": "app",
    "app": {
      "displayName": "LunchTable: School of Hard Knocks",
      "category": "game",
      "launchType": "connect",
      "launchUrl": "https://lunchtable.app",
      "capabilities": [
        "observe",
        "control",
        "story-mode",
        "pvp",
        "spectate",
        "chat"
      ],
      "minPlayers": 1,
      "maxPlayers": 2,
      "viewer": {
        "url": "https://lunchtable.app",
        "embedParams": {
          "embedded": "true",
          "mode": "play"
        },
        "postMessageAuth": true,
        "sandbox": "allow-scripts allow-same-origin allow-popups allow-forms"
      }
    }
  },
  "scripts": {
    "dev": "vite dev --port 3334",
    "build": "vite build && tsc --noEmit",
    "build:vercel": "vite build",
    "type-check": "tsc --noEmit",
    "preview": "vite preview --port 3334",
    "start": "node .output/server/index.mjs"
  },
  "dependencies": {
    "@convex-dev/react-query": "^0.1.0",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-router": "^1.162.4",
    "@tanstack/react-router-devtools": "^1.162.4",
    "@tanstack/react-router-ssr-query": "^1.162.4",
    "@tanstack/react-start": "^1.162.4",
    "convex": "^1.31.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.5.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.6.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.2",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
